<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="dateFoo" class="com.learn.spring.cotainer.starter.DateFoo">
        <property name="date">
            <value>2007/10/16</value>
        </property>
    </bean>


    <bean id="datePropertyEditor" class="com.learn.spring.cotainer.starter.DatePropertyEditor">
        <property name="datePattern">
            <value>yyyy/MM/dd</value>
        </property>
    </bean>

    <!--<bean id="datePropertyEditorRegistrar" class="">

    </bean>-->
</beans>