<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.learn.spring.cotainer.starter.beanpostprocessor.PasswordDecodePostProcessor">

    </bean>

    <bean id="password"
          class="com.learn.spring.cotainer.starter.beanpostprocessor.PasswordDecodableImpl">
        <property name="decodedPassword">
            <value>hello</value>
        </property>
    </bean>
</beans>